{
  "name": "ipay-subgraph",
  "version": "1.0.0",
  "description": "Subgraph for IPay Registry - indexes IP asset listings and usage events on Avalanche Fuji",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "test": "graph test",
    "prepare:fuji": "mustache config/fuji.json subgraph.template.yaml > subgraph.yaml",
    "deploy:goldsky": "goldsky subgraph deploy ipay-subgraph/1.0.0 --path .",
    "create:local": "graph create --node http://localhost:8020/ ipay-subgraph",
    "deploy:local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 ipay-subgraph"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.79.0",
    "@graphprotocol/graph-ts": "^0.35.1"
  },
  "devDependencies": {
    "matchstick-as": "^0.6.0",
    "mustache": "^4.2.0"
  }
}
